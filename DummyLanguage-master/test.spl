{
    fibonacci(int n, var int result){
        int tmp = 42;
        if ( n == 0 ) then {
            result = 0;
        } else {
            if ( n == 1 ) then {
                result = 1;
            } else {
                fibonacci(n-1, result);
                tmp = result;
                fibonacci(n-2, result);
                result = result + tmp;
            }
        }
    }
